---
import "../styles/global.css";
const { title } = Astro.props;
---

<div class="flex flex-row justify-between items-center">
    <h1 class="text-2xl sm:text-3xl lg:text-4xl font-extrabold break-words">
        {title}
    </h1>
</div>

<style>
    h1 {
        color: rgb(255, 169, 69);
    }
</style>
